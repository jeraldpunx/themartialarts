@import url('https://fonts.googleapis.com/css?family=Montserrat:400,700');
@import url('https://fonts.googleapis.com/css?family=Roboto+Condensed:400,300,700');


*{
  margin: 0;
  padding: 0;
}

body{
  font-family: 'Montserrat', sans-serif;
}

.fullwidth{
  display: block;
  width: 100%;
  float: left;
}

.h1, .h2, .h3, h1, h2, h3{
  margin-top: 0;
  text-transform: uppercase;
}

h3{
  color: #f2671f;
  font-size: 18px;
  text-transform: uppercase;
  font-weight: bold;
}

p{
  font-size: 14px;
  color: #666666;
  font-weight: lighter;
}

a{
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

.container-fluid{
  padding: 0;
}

header{
  background: #160a47;
  border-bottom: 5px solid #9c0f5f;
}

ul{
  padding: 0;
}


.navbar ul{
  margin: 0;
}

.navbar ul li{
  list-style: none;
  display: inline-block;
  margin: 0;
  float: left;
}

.navbar ul li a{
  color: #cccccc;
  font-size: 18px;
  text-transform: uppercase;
  font-weight: normal;
  background: transparent;
  padding: 25px 30px;
  display: block;
  border-right: 1px solid #2b1b6d;
}

.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:focus, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > li > a:hover{
  background: #9c0f5f;
  color: #fff;
  text-decoration: none;
}

.navbar-default{
  background: transparent;
  padding: 0;
  margin: 0;
  border: 0;
  border-radius: 0;
}

.navbar-collapse{
  padding: 0;
  border-width: 0;
}

.navbar-toggle{
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out; 
}

/** responsive menu **/

@media(max-width:767px){

  .navbar ul li{
    display: block;
    width: 100%;
  }

  .navbar ul li a{
    font-size: 14px;
    padding: 10px 15px;
    border-right: 0;
    border-bottom: 1px solid #2b1b6d;
  }

}

/** responsive menu end **/

.btn{
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
  border-radius: 0;
  padding: 10px;
}



#user-header{
  text-align: right;
  padding: 11px 0;
}

#user-header .user-logo, #user-header #user-name{
  display: inline-block;
  vertical-align: middle;
}

#user-header #user-name{
  font-family: 'Roboto Condensed', sans-serif;
  color: #fff;
  font-size: 28px;
  text-transform: uppercase;
  margin-left: 1%;
}

#user-option .btn-info{
  background: transparent;
  border: 0;
}

#user-option .btn-info:hover{
  background: #f2671f;  
}

#user-option .btn:hover .icon-bar{
  background-color: #160a47;
}

#user-option{position: relative;}

#user-option .btn .icon-bar{
  background: #f2671f none repeat scroll 0 0;
    border-radius: 3px;
    display: block;
    height: 4px;
    margin: 4px 0;
    width: 32px;
    -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

#usermenu{
  background: #f2671f;
    height: auto;
    padding: 20px 20px 15px 20px;
    position: absolute;
    right: 15px;
    top: 48px;
    width: 220px;
}

#usermenu ul{
  text-align: left;
}

#usermenu ul li{
  display: block;
}

#usermenu ul li a{
  color: #fff;
  display: block;
  text-transform: uppercase;
  padding: 4px 0;
}

#usermenu ul li a:hover{
  color: #160a47;
  text-decoration: none;
}

.user-wrap{
  padding: 4px 0;
}

#sidebar-absolute{
  display: block;
  padding: 25px;
  width: 320px;
}

#search_student input{
  background: #f1f1f1 url(../images/magni-icon.png) no-repeat 93% 16px;
  border: 0;
  display: block;
  margin: 0 auto 15px;
  width: 100%;
  position: relative;
  font-size: 16px;
  color: #160a47;
  text-transform: uppercase;
  padding: 15px 10px; 
  border-radius: 0;
  box-shadow: 0;
}

#search_student select{
  font-size: 10px;
  color: #999;
  text-transform: uppercase;
  padding-right: 10px;
  border: 0;
  border-radius: 0;
}

#search_student{
  display: block;
  width: 100%;
  float: left;
  
  
}

.btn{
  -webkit-transition: all 0.3s ease-in-out; 
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

#studentsearch .btn{
  color: #999;
  text-transform: uppercase;
  font-size: 10px;
  border: none;
  border-radius: 0;
  background: none;
  display: inline-block;
  margin-left:10px  ;
}

#studentsearch .btn:hover{
  color: #9c0f5f;
  font-weight: bold;
}

.wrapbtns{
  text-align: right;
  display: inline-block;
  float: right;
}

#searchresult li{
  display: block;
  list-style: none;
  font-size: 16px;
  color: #333;
  text-transform: uppercase;
  
  border-bottom: 1px solid #eaeaea;
  padding: 10px 0 20px 10px;

  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;

  margin-bottom: 10px;
}

#searchresult li:hover{
  background-color: #f8f8f8;
}

#searchresult li span{
  display: block;
}


#searchresult li span.stud-name{
  color: #333;
  font-size: 16px;
  text-transform: uppercase;
}
#searchresult li span.stud-address{
  color: #999;
  font-size: 11px;
  text-transform: uppercase;
}

#searchresult ul{
  padding: 0;
}

#searchresult{
  display: block;
  margin: 20px 0;
  float: left;
  padding-top: 24px;
  width: 100%;
  border-bottom: 1px solid #9c0f5f;
  border-top: 1px solid #9c0f5f;
}

.searchreport{
  display: block;
  margin-top: 40px;
}

.searchreport p{
  color: #999999;
  font-size: 12px;
  text-transform: uppercase;
}

.searchreport p span{
  color: #9c0f5f;
  font-size: 14px;
  font-weight: bold;
}

#add_stud .btn{
  float: right;
  background: #f2671f;
  border-radius: 0;
  color: #fff;
  font-size: 14px;
  border: 0;
  text-transform: uppercase;
   padding: 12px 35px;
}

#main-content{
  background: #fbfbfb;
  margin-top: 24px;
  padding: 30px 30px 50px 30px;
}

body.loginpage{
  background: url(../images/bg.jpg) no-repeat;
  background-size: cover;
  height: 68vh;

}

#loginform-wrap{
  display: block;
  max-width: 400px;
  width: 100%;
  margin: 20% auto 0;
  text-align: right;
  background: rgba(255,255,255,0.5);
  padding: 40px;
}

#loginform-wrap input{
  width: 100%;
  border: 1px solid #fff;
  background: #fbfbfb;
  padding: 15px;
  text-align: left;
  margin: 0 auto 15px;
  display: inline-block;
  float: none;
  color: #999;
}

#loginform-wrap input[type="submit"]{
  width: 50%;
  margin: 0;
  color: #fff;
  background-color: #160a47;
  border: 0;
  text-align: center;
  text-transform: uppercase;
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

#loginform-wrap input[type="submit"]:hover{
  background: #9c0f5f;
}

#loginform-wrap h4{
  text-transform: uppercase;
  color: #160a47;
  margin: 0 auto 20px;
  width: 100%;
  display: block;
  text-align: left;
  font-size: 24px;
}

#profile-info .fixwidth{
  width: 300px; 
}

.stud-name{
  font-size: 30px;
  color: #160a47;
  margin-bottom: 0;
}

.stud-location{
  font-size: 18px;
  color: #999999;
  text-transform: uppercase;
}

.profile-pic{
  display: block;
  background: #fff;
  border: 1px solid #e3e2e8;
}

.profile-pic img{
  width: 100%;
}

.cta-btn .fa{
  margin-right: 5px;
  font-size: 16px;
}

.cta-btn{
  display: inline-block;
  color: #fff;
  padding: 16px 24px;
  font-size: 14px;
  text-transform: uppercase;
}

.cta-orange{
  background: #f2671f;
}

.cta-purple{
  background: #9c0f5f;
  margin-left: 18px;
}

.info-header, .info-cta{
  display: inline-block;
  vertical-align: middle;
}

.info-cta{float: right;}

.contenttext{
  display: block;
  padding: 40px 0 0;
}

.contenttext h3{
  background: url(../images/h3-bg.jpg) repeat-x bottom;
  padding-bottom: 8px;
}

.contenttext textarea{
  width: 100%;
  border: 1px solid #f6f6f6;
  background: #fff;
  min-height: 150px;
  padding: 20px;
}

#footer{
  display: block;
  width: 100%;
  float: right;
  margin-top: 100px;
  border-top: 1px solid #eaeaea;
  padding-top:10px;
}

#footer p{
  font-size: 12px;
  text-transform: uppercase;
  color: #999;
}

main .sidebar{
  display: block;
  padding: 25px;
}

#filterLocation, #filterBranch{
    border: 1px solid #f1f1f1;
    border-radius: 0;
    color: #160a47;
    font-size: 11px;
    padding: 4px;
    text-transform: uppercase;
    width: 100%;
    margin-bottom: 11px;
}

input[type="checkbox"], input[type="radio"], .radiogroup{
   color: #160a47;
    font-size: 11px;
    margin-right: 5px;
     text-transform: uppercase;
}

#filters .radiogroup{
    display: block;
    
    width: 100%;
    text-align: right;
}

.radiogroup span{
  display: inline-block;
  margin-left: 4%;
}

#searchresult .profile-picture{
  text-align: left;
  float: left;
  margin-right: 10px;
  display: block;
  width: 50px;
  height: auto;
}

h1.maintitle{
  color: #160a47;
  font-size: 24px;
  text-transform: uppercase;
  background: url(../images/border-bottom.jpg) bottom repeat-x;
  margin-bottom: 25px;
    padding-bottom: 20px;
}

h1.maintitle span{
  color: #999999;
  font-size: 16px;
}

.form-group{
  padding: 0 15px 0 0;
}

input[type="text"], input[type="password"], .ui-autocomplete-input, textarea, .uneditable-input{
  background: #fff;
  border: 1px solid #f3f3f3;
  border-radius: 0;
  height: auto;
  padding: 15px;
  color: #777777;
  font-size: 12px;
  text-transform: uppercase;
  box-shadow: none;
}

.form-control:focus{
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(156, 15, 95, 0.6);
}

#profileupdate img{
    display: block;
    float: left;
    height: 274px;
    width: 230px;
}

.padding-30{
  display: block;
  float: left;
  height: 30px;
  width: 100%;
  clear: both;
}

.btn{
    border: 0 none;
    border-radius: 0;
    color: #fff;
    display: inline-block;
    font-size: 14px;
    padding: 12px 35px;
    text-transform: uppercase;
}

.cta-group{
  text-align: right;
}

.btn.orange{
  background: #f2671f;
}

.btn.grey{
    background: #999999;
    margin-left: 20px;
}

#profileheader .col-sm-8{padding-left: 0;}
